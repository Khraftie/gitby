{
  "hosting": {
    "public": "Pages",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/",
        "destination": "/index.html"
      },
      {
        "source": "/404",
        "destination": "/404.html"
      },
      {
        "source":"/projects",
        "destination": "/projects.html"
      },
      {
        "source": "/progress",
        "destination": "/progress.html"
      },
      {
        "source": "/langs",
        "destination": "/languages.html"
      }
    ]
  }
}
